PODS_ROOT = ${SRCROOT}/Pods
ALWAYS_SEARCH_USER_PATHS = YES
PODS_HEADERS_SEARCH_PATHS = ${PODS_PUBLIC_HEADERS_SEARCH_PATHS}
OTHER_LDFLAGS = -ObjC -Wl,-no_compact_unwind -framework Foundation -framework MessageUI -framework QuartzCore -framework UIKit
HEADER_SEARCH_PATHS = ${PODS_HEADERS_SEARCH_PATHS}
PODS_PUBLIC_HEADERS_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/BSKeyboardControls" "${PODS_ROOT}/Headers/BSModalPickerView" "${PODS_ROOT}/Headers/BlocksKit" "${PODS_ROOT}/Headers/MTBlockTableView" "${PODS_ROOT}/Headers/SEHumanizedTimeDiff" "${PODS_ROOT}/Headers/SVPullToRefresh" "${PODS_ROOT}/Headers/libffi"
PODS_BUILD_HEADERS_SEARCH_PATHS = "${PODS_ROOT}/BuildHeaders" "${PODS_ROOT}/BuildHeaders/BSKeyboardControls" "${PODS_ROOT}/BuildHeaders/BSModalPickerView" "${PODS_ROOT}/BuildHeaders/BlocksKit" "${PODS_ROOT}/BuildHeaders/MTBlockTableView" "${PODS_ROOT}/BuildHeaders/SEHumanizedTimeDiff" "${PODS_ROOT}/BuildHeaders/SVPullToRefresh" "${PODS_ROOT}/BuildHeaders/libffi"